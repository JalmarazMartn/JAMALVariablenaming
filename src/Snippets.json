	"AL repeat until with document lines": {
		"scope": "al",
		"prefix": "trepeatDocLines",
		"body": [
			"${1:SalesLine}.Reset();",
			"${1:SalesLine}.SetRange(\"${2:Document Type}\",${3:SalesHeader}.\"${2:Document Type}\");",
			"${1:SalesLine}.SetRange(\"Document No.\",${3:SalesHeader}.\"No.\");",
			"if ${1:SalesLine}.FindSet() then",
			"    repeat",
			"    until ${1:SalesLine}.Next() = 0;"
